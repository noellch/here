{
  "expo": {
    "name": "Here",
    "slug": "here",
    "version": "1.0.0",
    "scheme": "here",
    "platforms": ["ios"],
    "ios": {
      "bundleIdentifier": "com.here.app",
      "supportsTablet": false,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Here uses your location to show nearby people on the map.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Here uses your location to show nearby people on the map."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "10.18.3"
        }
      ]
    ],
    "extra": {
      "router": {
        "root": "./src/app"
      },
      "eas": {
        "projectId": "f200cc51-ef36-4175-a2f0-a640d157ff5a"
      }
    }
  }
}
