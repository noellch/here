{
  "expo": {
    "name": "Here",
    "slug": "here",
    "version": "1.0.0",
    "scheme": "here",
    "platforms": ["ios"],
    "ios": {
      "bundleIdentifier": "com.here.app",
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Here uses your location to show nearby people on the map."
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Here uses your location to show nearby people on the map."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.13.4"
        }
      ]
    ]
  }
}
