{
  "name": "here",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "oxlint ./src",
    "lint:fix": "oxlint --fix ./src",
    "format": "oxfmt ./src",
    "format:check": "oxfmt --check ./src",
    "version:patch": "npm version patch --no-git-tag-version && node -e \"const fs=require('fs');const p=JSON.parse(fs.readFileSync('package.json','utf8'));const a=JSON.parse(fs.readFileSync('app.json','utf8'));a.expo.version=p.version;fs.writeFileSync('app.json',JSON.stringify(a,null,2)+'\\n')\"",
    "version:minor": "npm version minor --no-git-tag-version && node -e \"const fs=require('fs');const p=JSON.parse(fs.readFileSync('package.json','utf8'));const a=JSON.parse(fs.readFileSync('app.json','utf8'));a.expo.version=p.version;fs.writeFileSync('app.json',JSON.stringify(a,null,2)+'\\n')\"",
    "version:major": "npm version major --no-git-tag-version && node -e \"const fs=require('fs');const p=JSON.parse(fs.readFileSync('package.json','utf8'));const a=JSON.parse(fs.readFileSync('app.json','utf8'));a.expo.version=p.version;fs.writeFileSync('app.json',JSON.stringify(a,null,2)+'\\n')\"",
    "release": "git tag v$(node -p \"require('./package.json').version\") && echo \"Tagged v$(node -p \"require('./package.json').version\"). Push with: git push origin v$(node -p \"require('./package.json').version\")\""
  },
  "dependencies": {
    "@nanostores/react": "^1.0.0",
    "@react-native-firebase/app": "^23.8.6",
    "@react-native-firebase/auth": "^23.8.6",
    "@react-native-firebase/firestore": "^23.8.6",
    "@rnmapbox/maps": "^10.2.10",
    "expo": "~54.0.33",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-status-bar": "~3.0.9",
    "nanostores": "^1.1.0",
    "ngeohash": "^0.6.3",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@types/ngeohash": "^0.6.8",
    "@types/react": "~19.1.0",
    "lefthook": "^2.1.1",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "typescript": "~5.9.2"
  }
}
